<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<Group fx:id="root" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
   <children>
      <FlowPane fx:id="flowPane" alignment="CENTER" hgap="20000.0" prefHeight="720.0" prefWidth="1000.0">
         <children>
            <Label fx:id="labelTitle" alignment="BOTTOM_CENTER" prefHeight="210.0" prefWidth="239.0" text="GATE">
               <FlowPane.margin>
                  <Insets />
               </FlowPane.margin>
               <font>
                  <Font name="Krungthep" size="96.0" />
               </font>
            </Label>
            <VBox fx:id="startMenu" alignment="CENTER" prefHeight="387.0" prefWidth="334.0" spacing="30.0">
               <children>
                  <Button fx:id="newGameButton" focusTraversable="false" mnemonicParsing="false" onAction="#StartNewGame" prefHeight="45.0" prefWidth="150.0" text="New Game" />
                  <Button fx:id="continueButton" focusTraversable="false" mnemonicParsing="false" onAction="#ContinueGame" prefHeight="45.0" prefWidth="150.0" text="Continue..." />
                  <Button fx:id="exitButton" focusTraversable="false" mnemonicParsing="false" onAction="#ExitGame" prefHeight="45.0" prefWidth="150.0" text="Exit" />
               </children>
               <FlowPane.margin>
                  <Insets />
               </FlowPane.margin>
            </VBox>
            <Label fx:id="labelCredits" alignment="BOTTOM_CENTER" prefHeight="103.0" prefWidth="148.0" text="2017 Â© Credit Ozuk &amp; Q" />
         </children>
      </FlowPane>
   </children>
</Group>
